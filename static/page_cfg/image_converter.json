{
    "pageType": "flex",
    "sidebar": {
        "sections": [       
            {
                "header": "Input",
                "controls": [
                    {
                        "label": "Upload Image",
                        "type": "file",
                        "id": "uploadImageFile",
                        "accepts": "image/*",
                        "onLoad": "onImageUploaded"
                    }
                ]
            },
            {
                "header": "Output",
                "controls": [
                    {
                        "type": "button",
                        "id": "copyToClipboardButton",
                        "text": "Copy To Clipboard",
                        "click": "onCopyToClipboardButton"
                    },
                    {
                        "type": "button",
                        "id": "downloadZeeplevelButton",
                        "text": "Download .zeeplevel",
                        "click": "onDownloadZeeplevelButton"
                    }
                ]
            }
        ]
    },
    "panels": [
        {
            "type": "flex"
        }
    ],    
    "javascriptFile": "/js/pages/image_converter.js"
}
